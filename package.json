    "test:mocha": "mocha test -r babel-polyfill -r babel-register",
    "test:standard": "standard",
    "clean": "rm -rf lib/"
  },
  "keywords": [
    "async",
    "promise",
    "singleton",
    "throttle",
    "debounce"
